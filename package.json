{
  "name": "anndata.js",
  "version": "0.0.1",
  "author": "Ilan Gold",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilan-gold/anndata.js.git"
  },
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "lint": "biome ci .",
    "fix": "biome check --apply .",
    "fix-unsafe": "biome check  --apply-unsafe .",
    "publint": "pnpm exec publint"
  },
  "dependencies": {
    "zarrita": "0.4.0-next.12",
    "@zarrita/core": "0.1.0-next.10",
    "@zarrita/indexing": "0.1.0-next.12",
    "@zarrita/storage": "0.1.0-next.4",
    "@zarrita/typedarray": "0.1.0-next.2",
    "numcodecs": "^0.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "vitest": "^0.32.2",
    "publint": "^0.2.2",
    "typescript": "^4.9.5"
  }
}
